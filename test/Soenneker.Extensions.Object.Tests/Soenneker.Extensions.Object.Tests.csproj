<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <Nullable>enable</Nullable>
        <IsPackable>false</IsPackable>
        <UseMicrosoftTestingPlatform>false</UseMicrosoftTestingPlatform>
    </PropertyGroup>

    <ItemGroup>
        <None Remove="appsettings.json" />
    </ItemGroup>

    <ItemGroup>
        <Content Include="appsettings.json">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </Content>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.NET.Test.Sdk" Version="18.0.0" />
        <PackageReference Include="Soenneker.Dtos.StreetAddress" Version="4.0.473" />
        <PackageReference Include="Soenneker.Facts.Local" Version="4.0.525" />
        <PackageReference Include="Soenneker.Facts.Manual" Version="4.0.158" />
        <PackageReference Include="Soenneker.Tests.Benchmark" Version="4.0.49" />
        <PackageReference Include="xunit.runner.visualstudio" Version="3.1.5">
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>
        <PackageReference Include="Soenneker.Tests.FixturedUnit" Version="4.0.2023" />
        <PackageReference Include="Soenneker.Utils.Test" Version="4.0.295" />
        <PackageReference Include="xUnit.v3" Version="3.1.0" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\src\Soenneker.Extensions.Object.csproj" />
    </ItemGroup>

</Project>